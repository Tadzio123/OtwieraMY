        url=\"https://api.maptiler.com/maps/bright/{z}/{x}/{y}.png?key=ABxrBA7sOVSSwxg7OTjT\"\r\n        />\r\n        {coordinates.map((place) => (\r\n          <Marker\r\n            key={place.id}\r\n            index={place.id}\r\n            position={[place.coordinateX, place.coordinateY]}\r\n            icon={getMarkerIcon(place.id)}\r\n            eventHandlers={{\r\n              click: () => setActiveMarker(place.id),\r\n            }}\r\n          />\r\n        ))}\r\n\r\n        {activeMarkerData && (\r\n          <StyledPopup\r\n            position={[\r\n              activeMarkerData.coordinateX,\r\n              activeMarkerData.coordinateY,\r\n            ]}\r\n            onClose={() => {\r\n              this.setState({\r\n                activeMarker: null,\r\n                activeMarkerData: null,\r\n              });\r\n            }}\r\n          >\r\n            <h1>{activeMarkerData.city}</h1>\r\n          </StyledPopup>\r\n        )}\r\n\r\n      </StyledMapContainer>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Map;\r\n","C:\\Users\\Tadzio\\Desktop\\OtwieraMY\\src\\components\\molecules\\Markers\\index.js",[],"C:\\Users\\Tadzio\\Desktop\\OtwieraMY\\src\\components\\molecules\\Popup\\index.js",[],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"110","replacedBy":"122"},{"ruleId":"112","replacedBy":"123"},{"ruleId":"114","replacedBy":"124"},{"ruleId":"116","replacedBy":"125"},{"ruleId":"118","replacedBy":"126"},{"ruleId":"120","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":3,"column":36,"nodeType":"130","messageId":"131","endLine":3,"endColumn":41},"lines-around-directive",["132"],"no-spaced-func",["133"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],["132"],["133"],[],[],[],[],"no-unused-vars","'Popup